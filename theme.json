{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"border": {
			"radius": true,
			"color": true,
			"style": true,
			"width": true
		},
		"color": {
			"palette": [
				{
					"slug": "foreground",
					"color": "#1D201F",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "#f2f2f2",
					"name": "Background"
				},
				{
					"slug": "primary",
					"color": "#FFDE0D",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#fffded",
					"name": "Secondary"
				},

				{
					"slug": "tertiary",
					"color": "#ddd",
					"name": "Tertiary"
				},
				{
					"slug": "quaternary",
					"color": "#666666",
					"name": "Quaternary"
				},
				{
					"slug": "accent",
					"color": "#d30d1d",
					"name": "Accent"
				}
			]
		},
		"custom": {
			"spacing": {
				"baseline": "16px",
				"small": "min(40px, 8vw)",
				"medium": "calc(2.5 * var(--wp--custom--spacing--small))",
				"large": "min(3rem, 4vw)",
				"gap": {
					"horizontal": "clamp( calc( 1.5 * var( --wp--custom--spacing--baseline ) ), 2.222vw, calc( 2 * var( --wp--custom--spacing--baseline ) ) )",
					"vertical": "clamp( calc( 1.5 * var( --wp--custom--spacing--baseline ) ), 3.333vw, calc( 3 * var( --wp--custom--spacing--baseline ) ) )"
				},
				"gutter": "clamp( calc( 1.5 * var( --wp--custom--spacing--baseline ) ), 3.333vw, calc( 3 * var( --wp--custom--spacing--baseline ) ) )",
				"outer": "var( --wp--custom--spacing--gutter )"
			},
			"typography": {
				"letter-spacing": {
					"tight": "-0.44px",
					"tighter": "-.55px",
					"loose": ".44px"
				},
				"line-height": {
					"body": 1.4,
					"headings": {
						"gigantic": 1.2,
						"large": 1.2
					}
				}
			}
		},
		"layout": {
			"contentSize": "640px",
			"wideSize": "1600px"
		},
		"spacing": {
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"typography": {
			"customFontSize": true,
			"fontFamilies": [
				{
					"fontFamily": "ui-sans-serif, -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", sans-serif",
					"slug": "sans",
					"name": "Sans"
				},
				{
					"fontFamily": "EBGaramond, ui-serif, Georgia, serif",
					"slug": "serif",
					"name": "Serif",
					"fontFace": [
						{
							"fontFamily": "EBGaramond",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400 800",
							"src": [
								"file:./assets/fonts/EBGaramond/EBGaramond-VariableFont_wght.ttf"
							]
						},
						{
							"fontFamily": "EBGaramond",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400 800",
							"src": [
								"file:./assets/fonts/EBGaramond/EBGaramond-Italic-VariableFont_wght.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "ui-monospace, Menlo, Consolas, Monaco, \"Liberation Mono\", \"Lucida Console\", monospace",
					"slug": "mono",
					"name": "Mono"
				}
			],
			"fontSizes": [
				{
					"slug": "tiny",
					"size": "clamp(0.675rem, 0.8182rem + 0.1818vw, 0.875rem)",
					"name": "Tiny"
				},
				{
					"slug": "small",
					"size": "clamp(1rem, 0.9432rem + 0.1818vw, 1.125rem)",
					"name": "Small"
				},
				{
					"slug": "medium",
					"size": "clamp(1.0625rem, 0.9489rem + 0.3636vw, 1.3125rem)",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "clamp(1.3125rem, 1.2273rem + 0.2727vw, 1.5rem)",
					"name": "Large"
				},
				{
					"slug": "extra-large",
					"size": "clamp(1.5rem, 2.1335rem + 2.0273vw, 1.6rem)",
					"name": "Extra Large"
				},
				{
					"slug": "gigantic",
					"size": "clamp(4rem, 3.0909rem + 2.9091vw, 6rem)",
					"name": "Gigantic"
				},
				{
					"slug": "heading-1",
					"size": "clamp(1.75rem, 1.2955rem + 1.4545vw, 2.75rem)",
					"name": "Heading 1"
				},
				{
					"slug": "heading-2",
					"size": "clamp(1.625rem, 1.4545rem + 0.5455vw, 2.2rem)",
					"name": "Heading 2"
				},
				{
					"slug": "heading-3",
					"size": "clamp(1.5rem, 1.3295rem + 0.5455vw, 1.875rem)",
					"name": "Heading 3"
				},
				{
					"slug": "heading-4",
					"size": "clamp(1.375rem, 1.2045rem + 0.5455vw, 1.75rem)",
					"name": "Heading 4"
				},
				{
					"slug": "heading-5",
					"size": "clamp(1.25rem, 1.1364rem + 0.3636vw, 1.5rem)",
					"name": "Heading 5"
				},
				{
					"slug": "heading-6",
					"size": "clamp(1.125rem, 1.0682rem + 0.1818vw, 1.25rem)",
					"name": "Heading 6"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"spacing": {
			"blockGap": "var( --wp--custom--spacing--small )"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--serif)",
			"fontSize": "var(--wp--preset--font-size--large)",
			"letterSpacing": "var(--wp--custom--typography--letter-spacing--body)",
			"lineHeight": "var(--wp--custom--typography--line-height--body)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)",
					"fontSize": "var(--wp--preset--font-size--heading-1)",
					"fontWeight": "800",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing-tighter)",
					"lineHeight": "var(--wp--custom--typography--line-height--headings--gigantic)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)",
					"fontSize": "var(--wp--preset--font-size--heading-2)",
					"fontWeight": "700",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing-tighter)",
					"lineHeight": "var(--wp--custom--typography--line-height--headings--gigantic)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)",
					"fontSize": "var(--wp--preset--font-size--heading-3)",
					"fontWeight": "600",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--tighter)",
					"lineHeight": "var(--wp--custom--typography--line-height--headings--gigantic)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)",
					"fontSize": "var(--wp--preset--font-size--heading-4)",
					"fontWeight": "500",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--tighter)",
					"lineHeight": "var(--wp--custom--typography--line-height--headings--large)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)",
					"fontSize": "var(--wp--preset--font-size--heading-5)",
					"fontWeight": "500",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--tighter)",
					"lineHeight": "var(--wp--custom--typography--line-height--headings--large)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)",
					"fontSize": "var(--wp--preset--font-size--heading-6)",
					"fontWeight": "500",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--tighter)",
					"lineHeight": "var(--wp--custom--typography--line-height--headings--large)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--accent)"
				}
			}
		},
		"blocks": {
			"core/group": {
				"spacing": {
					"margin": {
						"bottom": "var( --wp--custom--spacing--outer )",
						"top": "var( --wp--custom--spacing--outer )"
					}
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--foreground)"
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"letterSpacing": "-1px",
					"fontWeight": "700",
					"fontFamily": "var(--wp--preset--font-family--sans)",
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/separator": {
				"border": {
					"width": "2px"
				}
			},
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/query": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--foreground)"
						},
						"typography": {
							"textDecoration": "none",
							"letterSpacing": "var(--wp--custom--typography--letter-spacing--tighter)"
						}
					},
					"h2": {
						"spacing": {
							"margin": {
								"top": "calc(var(--wp--custom--spacing--baseline)/2)"
							}
						},
						"typography": {
							"fontSize": "var(--wp--preset--font-size--large)"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--tiny)",
					"fontFamily": "var(--wp--preset--font-family--sans)"
				},
				"color": {
					"text": "var(--wp--preset--color--quaternary)"
				}
			},
			"core/post-excerpt": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				},
				"color": {
					"text": "var(--wp--preset--color--quaternary)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans)",
					"fontSize": "var(--wp--preset--font-size--tiny)"
				},
				"color": {
					"text": "var(--wp--preset--color--quaternary)"
				}
			},
			"core/post-author": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans)",
					"fontSize": "var(--wp--preset--font-size--tiny)"
				},
				"color": {
					"text": "var(--wp--preset--color--quaternary)"
				}
			}
		}
	}
}
